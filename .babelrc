{
  "plugins": [
    [
      "babel-plugin-styled-components",
      {
        "ssr": false
      }
    ],
    "@babel/plugin-proposal-class-properties"
  ],
  "presets": [
    [
      "@babel/env",
      {
        "targets": {
          "node": "6.11.5"
        },
        "spec": true,
        "useBuiltIns": "usage"
      }
    ],
    "@babel/react"
  ]
}